<%- model_class = DetailOrder -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:order_id) %>:</strong></dt>
  <dd><%= @detail_order.order_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:product_id) %>:</strong></dt>
  <dd><%= @detail_order.product_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:song_id) %>:</strong></dt>
  <dd><%= @detail_order.song_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cntProd) %>:</strong></dt>
  <dd><%= @detail_order.cntProd %></dd>
  <dt><strong><%= model_class.human_attribute_name(:subtotal) %>:</strong></dt>
  <dd><%= @detail_order.subtotal %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              detail_orders_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_detail_order_path(@detail_order), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              detail_order_path(@detail_order),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
