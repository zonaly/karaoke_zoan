<%- model_class = DetailVoucher -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:voucher_id) %>:</strong></dt>
  <dd><%= @detail_voucher.voucher_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:detail_order_id) %>:</strong></dt>
  <dd><%= @detail_voucher.detail_order_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cantProd) %>:</strong></dt>
  <dd><%= @detail_voucher.cantProd %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              detail_vouchers_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_detail_voucher_path(@detail_voucher), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              detail_voucher_path(@detail_voucher),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
